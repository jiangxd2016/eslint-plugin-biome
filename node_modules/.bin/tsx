#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules/tsx/dist/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules/tsx/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/tsx/dist/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/tsx/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules/tsx/dist/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules/tsx/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/tsx@4.7.1/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/tsx/dist/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/tsx/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsx/dist/cli.mjs" "$@"
else
  exec node  "$basedir/../tsx/dist/cli.mjs" "$@"
fi
