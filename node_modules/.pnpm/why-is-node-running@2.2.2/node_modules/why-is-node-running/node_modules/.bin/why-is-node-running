#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules/why-is-node-running/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules/why-is-node-running/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/why-is-node-running@2.2.2/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules/.pnpm/node_modules:/Users/xd/Documents/www/eslint-plugin-biome/node_modules:/Users/xd/Documents/www/node_modules:/Users/xd/Documents/node_modules:/Users/xd/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
